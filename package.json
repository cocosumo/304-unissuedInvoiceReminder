{
    "name": "app-304",
    "version": "1.0.0",
    "description": "",
    "main": "src/app.js",
    "scripts": {
        "envexample": "generate-env-example",
        "dtsgen:unissuedInvReminder": "dotenv -- npx kintone-dts-gen --app-id 303 --namespace UnissuedInvReminder -o types/unissuedInvReminder.d.ts",
        "dtsgen:projects": "dotenv -- npx kintone-dts-gen --app-id 209 --namespace Projects -o types/projects.d.ts",
        "dtsgen:contracts": "dotenv -- npx kintone-dts-gen --app-id 232 --namespace Contracts -o types/contracts.d.ts",
        "dtsgen:stores": "dotenv -- npx kintone-dts-gen --app-id 19 --namespace Stores -o types/stores.d.ts",
        "dtsgen:employees": "dotenv -- npx kintone-dts-gen --app-id 34 --namespace Employees -o types/employees.d.ts",
        "dtsgen:projTypes": "dotenv -- npx kintone-dts-gen --app-id 190 --namespace ProjTypes -o types/projTypes.d.ts",
        "tsc": "tsc --noEmit --project tsconfig.json",
        "start": "node built/index.js",
        "build": "esbuild src/app.ts --bundle --minify --define:process.env.NODE_ENV='production' --outfile=dist/customize.js",
        "build-dev": "esbuild src/app.ts --bundle --minify --define:process.env.NODE_ENV='production' --outfile=dist/customize.js --watch",
        "dev": "esbuild src/app.ts --bundle --define:process.env.NODE_ENV='development' --minify=false --outfile=dist/customize.js  --watch",
        "upload:local": "dotenv -- npx kintone-customize-uploader dest/kintone.local.json",
        "upload:prod": "dotenv -- npx kintone-customize-uploader dest/kintone.prod.json",
        "deploy": "npm run build && run upload:prod"
    },
